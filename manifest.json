{
  "name": "Google bookmarks",
  "version": "3.0.0",
  "description": "Google bookmarks extension for Chrome",
  "background_page": "background/background.html",
  "options_page": "options/options.html",
  "icons": {
  	"128":"public/folder128.png",
  	"16":"public/folder16.png",
  	"32":"public/folder32.png",
  	"48":"public/folder48.png"
  },
  "browser_action": {
    "default_icon": "public/folder32.png",
		"default_title": "Google Bookmarks",
		"popup": "ui/bookmark.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "http://www.google.com/"
  ]

}
